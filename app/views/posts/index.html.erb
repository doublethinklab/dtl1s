<%= flash[:notice] %>
<h4>Import</h4>
<%= form_tag import_posts_path, multipart: true do %>
    <%= file_field_tag :file %>
    <%= submit_tag "Import CSV from CrowdTangle" %>
<% end %>
<br>
<%= button_to "CrowdTangle API UPDATE", api_posts_path, method: :get %>

<table>
<% @posts.each do |post| %>
<tr>
    <td> <%= post.node.name %></td>
      <td><%= post.title %></td>
      <td> <%= post.url %></td>
      <td> <%= post.link %></td>
    </tr>
<% end %>
</table>
